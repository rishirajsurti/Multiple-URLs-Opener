{
  "manifest_version": 2,

  "name": "Multiple URLs Opener",
  "description": "Open set of URLs in dedicated windows",
  "version": "1.0",
  "author": "Rishiraj Surti",

  "browser_action": {
   "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",  
      "48": "icons/icon48.png",     
      "64": "icons/icon64.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png",
      "512": "icons/icon512.png"     
    }
  },
  "icons": { 
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",  
      "48": "icons/icon48.png",     
      "64": "icons/icon64.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png",
      "512": "icons/icon512.png"       
    },

  "permissions": [
   "tabs", "activeTab"
    ],
  "background": {
    "scripts": ["jquery-2.2.4.min.js", "URI.js", "index.js" ],
    "persistent": false
  }
}